<launch>
    <include file="$(find image_compressor)/launch/decmpr.launch"/>

    <node pkg="rosbag" type="play" name="player" output="log" args="-l $(find image_compressor)/src/test/auto_record_2020-12-28-16-55-14_29_jpg.bag">
    </node>

    <test name="publishtest" test-name="publishtest" pkg="rostest" type="publishtest">
        <rosparam>
          topics:
            - name: /cam/front_bottom_60/raw
              timeout: 15
              negative: False
            - name: /cam/front_top_far_30/raw
              timeout: 15
              negative: False
            - name: /cam/front_top_close_120/raw
              timeout: 15
              negative: False
            - name: /cam/right_front_60/raw
              timeout: 15
              negative: False
            - name: /cam/right_back_60/raw
              timeout: 15
              negative: False
            - name: /cam/left_front_60/raw
              timeout: 15
              negative: False
            - name: /cam/left_back_60/raw
              timeout: 15
              negative: False
            - name: /cam/back_top_120/raw
              timeout: 15
              negative: False
        </rosparam>
    </test>
</launch>
