<launch>
    <include file="$(find edge_detection)/launch/edge_detection.launch" />

    <node pkg="rosbag" type="play" name="player" output="log" args="$(find car_model)/../bags/edge_detection_2020-04-13-17-45-48_0.bag" />

    <test name="publishtest" test-name="publishtest" pkg="rostest" type="publishtest">
        <rosparam>
            topics:
                - name: /occupancy_grid
                  timeout: 15
                  negative: False
                - name: /ring_edge_point_cloud
                  timeout: 15
                  negative: False
        </rosparam>
    </test>
</launch>
