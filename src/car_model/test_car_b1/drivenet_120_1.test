<launch>
    <!-- drivenet_120_1 !-->
		<node name="drivenet_node1" pkg="drivenet" type="drivenet_node1"  output="screen">
						<param name="car_id" type="int" value="1" />
						<param name="standard_fps" type="bool" value="0" />
						<param name="display" type="bool" value="0" />
						<param name="input_resize" type="bool" value="1" />
						<param name="imgResult_publish" type="bool" value="1" />
		</node>

    <node pkg="rosbag" type="play" name="player" output="screen" args="$(find car_model)/../bags/my_record_2019-11-27-16-36-54.bag" required="true"/>

    <test name="publishtest" test-name="publishtest" pkg="rostest" type="publishtest">
        <rosparam>
          topics:
            - name: /cam/R_front/detect_image
              timeout: 60
              negative: False
            - name: /cam/R_rear/detect_image
              timeout: 60
              negative: False
            - name: /cam/L_front/detect_image
              timeout: 60
              negative: False
            - name: /cam/L_rear/detect_image
              timeout: 60
              negative: False
        </rosparam>
    </test>
</launch>
