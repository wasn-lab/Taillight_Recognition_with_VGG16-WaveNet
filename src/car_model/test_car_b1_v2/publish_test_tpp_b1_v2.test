<launch>
    <include file="$(find itri_tracking_pp)/launch/tpp.launch"/>

    <node pkg="rosbag" type="play" name="player" output="log" args="$(find car_model)/../bags/auto_record_2020-03-10-10-48-39_41.bag" required="true"/>

    <test name="publishtest" test-name="publishtest" pkg="rostest" type="publishtest">
        <rosparam>
            topics:
                - name: /PathPredictionOutput
                  timeout: 15
                  negative: False
                - name: /PathPredictionOutput/grid
                  timeout: 15
                  negative: False
                - name: /PathPredictionOutput/id
                  timeout: 15
                  negative: False
                - name: /PathPredictionOutput/markers
                  timeout: 15
                  negative: False
                - name: /PathPredictionOutput/pp
                  timeout: 15
                  negative: False
                - name: /PathPredictionOutput/radar
                  timeout: 15
                  negative: False
                - name: /PathPredictionOutput/speed
                  timeout: 15
                  negative: False
                - name: /PathPredictionOutput/vel
                  timeout: 15
                  negative: False
        </rosparam>
    </test>
</launch>
