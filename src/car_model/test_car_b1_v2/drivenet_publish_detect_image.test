<launch>
    <include file="$(find drivenet)/launch/b1_v2_drivenet_group_a.launch"/>
    <include file="$(find drivenet)/launch/b1_v2_drivenet_group_b.launch"/>
    <include file="$(find drivenet)/launch/b1_v2_drivenet_group_c.launch"/>

    <node pkg="rosbag" type="play" name="player" output="log" args="$(find car_model)/../bags/auto_record_2020-03-10-10-48-39_41.bag" required="true"/>

    <test name="publishtest" test-name="publishtest" pkg="rostest" type="publishtest">
        <rosparam>
          topics:
            - name: /cam/front_bottom_60/detect_image
              timeout: 15
              negative: False
            - name: /cam/front_top_far_30/detect_image
              timeout: 15
              negative: False
            - name: /cam/front_top_close_120/detect_image
              timeout: 15
              negative: False
            - name: /cam/right_front_60/detect_image
              timeout: 15
              negative: False
            - name: /cam/right_back_60/detect_image
              timeout: 15
              negative: False
            - name: /cam/left_front_60/detect_image
              timeout: 15
              negative: False
            - name: /cam/left_back_60/detect_image
              timeout: 15
              negative: False
            - name: /cam/back_top_120/detect_image
              timeout: 15
              negative: False
        </rosparam>
    </test>
</launch>
