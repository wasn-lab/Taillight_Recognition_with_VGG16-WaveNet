<launch>
    <include file="$(find itri_tracking_2d)/launch/track2d.launch"/>

    <node pkg="rosbag" type="play" name="player" output="log" args="$(find car_model)/../bags/auto_record_2020-08-04-10-15-25_4_filtered.bag" />

    <test name="publishtest" test-name="publishtest" pkg="rostest" type="publishtest">
        <rosparam>
            topics:
                - name: /Tracking2D/front_bottom_60
                  timeout: 15
                  negative: False
        </rosparam>
    </test>
</launch>
