<launch>
<!--
Work in progress, not add to publish_test.sh yet.
-->
    <include file="$(find itri_tracking_2d)/launch/track2d.launch"/>
    <include file="$(find itri_pedcross)/launch/ped.launch">
      <rosparam param="input_source">0</rosparam>
    </include>

    <node pkg="rosbag" type="play" name="player" output="log" args="$(find car_model)/../bags/auto_record_2020-04-14-16-41-15_89.bag" />

    <test name="publishtest" test-name="publishtest" pkg="rostest" type="publishtest">
        <rosparam>
            topics:
                - name: /Tracking2D
                  timeout: 15
                  negative: False
                - name: /PedCross/Alert
                  timeout: 15
                  negative: False
                - name: /PedCross/DrawBBox
                  timeout: 15
                  negative: False
<!--
                - name: /PedCross/3D_marker
                  timeout: 15
                  negative: False
-->
        </rosparam>
    </test>
</launch>
