    <include file="$(find control)/launch/ukfmm.launch" />

    <test name="publishtest" test-name="publishtest" pkg="rostest" type="publishtest">
        <rosparam>
            topics:
                - name: /ukf_mm_topic
                  timeout: 15
                  negative: False
